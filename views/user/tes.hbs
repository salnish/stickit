    {{!-- Block UnBlock --}}
    <script>
        function unBlockUser(proId) {
            $.ajax({
                url: '/admin/unBlockUser/' + proId,
                method: 'get',
                success: (response) => {
                    if (response.status)
                        alert(response)
                }
            })
        }
    </script>
    <script>
